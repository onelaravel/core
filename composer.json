{
    "name": "onelaravel/core",
    "description": "One - Laravel Core Library for rapid development and security",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "One Team",
            "email": "support@One.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0|^12.0",
        "bacon/bacon-qr-code": "^3.0"
    },
    "require-dev": {
        "laravel/octane": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "One\\Core\\": "src/core/"
        },
        "files": [
            "src/helpers/__loader__.php"
        ],
        "classmap": [
            "src/config/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "One\\Core\\Providers\\OneServiceProvider"
            ],
            "aliases": {
                "One": "One\\Core\\Facades\\One"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
